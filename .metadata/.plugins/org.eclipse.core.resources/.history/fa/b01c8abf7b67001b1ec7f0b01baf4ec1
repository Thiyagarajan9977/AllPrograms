package org.cons;

public class Employee {

	public Employee() {
		this(int a);
		System.out.println("default constructor");
	}

	public Employee(int a) {
		

		System.out.println("Int Parameterized Constructor");

	}

	public static void main(String[] args) {

		Employee c = new Employee();
		new Employee();
		

	}

}
