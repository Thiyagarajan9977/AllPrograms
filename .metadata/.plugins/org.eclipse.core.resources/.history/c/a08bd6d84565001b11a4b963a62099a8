package org.string;

public class Q916 {
	
	public static void main(String[] args) {
		
		String st = "Welcome";
	
		for (int i = 0; i < st.length(); i++) {
			
			if (st.charAt(i) == 'a' || st.charAt(i) == 'e' || st.charAt(i) == 'i' || st.charAt(i) == 'o'
					|| st.charAt(i) == 'u') {
				
			System.out.println(st.replace('a',  '@'));
			System.out.println(st.replace('e',  '@'));
			System.out.println(st.replace('i',  '@'));
			System.out.println(st.replace('o',  '@'));
			System.out.println(st.replace('u',  '@'));
			
			}
		}
	}
}
